<script>
	var uamdatatables = {
		ajax: {
			url: "{{ path('uam_maintenance_admin_list') }}"
		},

		columns: [
			{
				data: null,
				orderable: false,
				visible: false
			},
			{ data: "date_start" },
			{ data: "date_end" },
			{
				data: "description",
				orderable: false,
			},
			{
				data: function( row, type, val, meta ) {
					if ( "display" === type ) {
						var template = $( "template.confirmed" ).html();
						return Mustache.render( template, row );
					}

					return "";
				},
				orderable: false,
			},
			{
				data: function( row, type, val, meta ) {
					if ( "display" === type ) {
						var template = $( "template.actions" ).html();
						return Mustache.render( template, row );
					}

					return "";
				},
				orderable: false,
				className: "actions"
			}
		],
		searching: false,
	};
	$( ".datepicker" ).datetimepicker();
</script>